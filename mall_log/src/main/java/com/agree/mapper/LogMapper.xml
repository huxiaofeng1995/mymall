<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.agree.mapper.LogMapper">
	<insert id="log" parameterType="String" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into log(message)
		value (#{message})
	</insert>
</mapper>
